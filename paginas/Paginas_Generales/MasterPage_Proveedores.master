            </div>
            <div id="contenido_fondo" runat="server">
                <asp:ContentPlaceHolder id="Cph_Area_Trabajo1" runat="server">
                </asp:ContentPlaceHolder>
            </div>            
            <div id="pie_login" runat="server" ></div>                
            <div id="pie" runat="server" visible="false"></div>            
        </div> 
    </div>
    </form>
</body>
</html>
